{
  "plugins": [
      "plugins/markdown"
  ],
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/jsdoc-template"
  },
  "templates": {
      "referenceTitle": "Wavecore",
      "disableSort": false,
      "collapse": true,
      "resources": {
          "hypercore v10": "https://github.com/hypercore-protocol/hypercore-next",
          "random-access-storage": "https://github.com/random-access-storage",
          "WaveFile": "https://github.com/rochars/wavefile"
      }
  },
  "source": {
    "include": ["index.js", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|demo)"
  }
}
