{
  "name": "@storyboard-fm/wavecore",
  "version": "0.1.2",
  "description": " Distributed peer-to-peer WAV files",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/browserify index.js -o demo/bundle.js",
    "docs": "node_modules/.bin/jsdoc --configure jsdoc.json --verbose",
    "lint": "node_modules/.bin/prettier -w index.js",
    "make-badges": "istanbul-badges-readme",
    "test": "nyc --reporter=json-summary --reporter=html --reporter=text mocha --timeout=30000"
  },
  "author": "agrathwohl",
  "license": "MIT",
  "dependencies": {
    "@storyboard-fm/little-media-box": "github:Storyboard-fm/little-media-box",
    "audio-buffer-from": "^1.1.1",
    "hypercore": "github:hypercore-protocol/hypercore-next",
    "hyperswarm": "^3.0.1",
    "multistream": "^4.1.0",
    "nanoprocess": "^0.8.5",
    "random-access-memory": "^4.0.0"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "chai": "^4.3.4",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "git-pre-hooks": "^1.2.1",
    "istanbul-badges-readme": "^1.8.1",
    "jsdoc": "^3.6.7",
    "jsdoc-template": "github:braintree/jsdoc-template",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "parcel": "^2.2.0",
    "prettier": "^2.5.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "git-pre-hooks": {
    "pre-commit": "npm run lint"
  }
}
